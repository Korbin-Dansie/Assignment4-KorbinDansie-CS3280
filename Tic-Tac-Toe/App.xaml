<Application x:Class="Tic_Tac_Toe.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Tic_Tac_Toe"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <Color x:Key="oneDark">#780000</Color>
        <Color x:Key="oneLight">#C1121F</Color>
        <Color x:Key="mainColor">#FDF0D5</Color>
        <Color x:Key="mainDark">#FFDE9B</Color>
        <Color x:Key="mainDarkTwo">#FFBF3D</Color>
        <Color x:Key="twoDark">#003049</Color>
        <Color x:Key="twoLight">#669BBC</Color>
        <Color x:Key="twoMid">#4C88AD</Color>
        <Color x:Key="circleBlue">#FF1BB1E4</Color>

        <Color x:Key="colorWhite">#FFFFFF</Color>

        <Color x:Key="activePlayer">#B8CCB8</Color>

    </Application.Resources>
</Application>
